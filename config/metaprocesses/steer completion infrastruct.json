{
    "description": "Continues the branch text with instructions",
    "input_transform": "lambda input: input[-6000:]",
    "prompt_template": "lambda input, aux_input: f\"{metaprocess_headers['infrastruct']}\\n\\n[user](#context)\\n{input[0:-100]}\\n\\n[user](#message)\\n{aux_input}\\n\\n[assistant](#continuation)\\n{input[-100:]}\"",
    "output_transform": "lambda output: get_completion_branches(output)",
    "generation_settings": {
        "engine": "gpt-4-base",
        "n": 4,
        "max_tokens": 100,
        "logprobs": 0,
        "temperature": 1
    },
    "output_type": "completions"
}